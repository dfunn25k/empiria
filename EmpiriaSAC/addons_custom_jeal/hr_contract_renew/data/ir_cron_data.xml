<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <record id="ir_cron_hr_contract_auto_renewal" model="ir.cron">
            <field name="name">Procesar Renovaciones de Contratos</field>
            <field name="model_id" ref="model_hr_contract_renew"/>
            <field name="state">code</field>
            <field name="code">model._cron_process_pending_renewals()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="numbercall">-1</field>
            <field name="doall" eval="True"/>
            <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-%d 23:10:00')"/>
        </record>
    </data>
</odoo>
