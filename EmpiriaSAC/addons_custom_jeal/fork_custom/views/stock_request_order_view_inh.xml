<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="stock_request_order_view_form_inh" model="ir.ui.view">
            <field name="name">stock.request.order.view.form.inh</field>
            <field name="model">stock.request.order</field>
            <field name="inherit_id" ref="stock_request.stock_request_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='items']/field/tree/field[@name='product_id']" position="after">
                    <field name="marca"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
